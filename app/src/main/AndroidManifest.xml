<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.livejournal.karino2.openeibunpou" >

    <uses-permission android:name="android.permission.GET_ACCOUNTS" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.USE_CREDENTIALS" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity android:name=".StageActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".QuestionActivity" >
        </activity>
        <activity android:name=".AnswerActivity" >
        </activity>
        <activity android:name=".FinishStageActivity" >
        </activity>
        <activity android:name=".SetupActivity" >
        </activity>
        <activity android:name=".UserPostActivity" >
        </activity>
        <activity
            android:name=".LatestUserPostActivity"
            android:label="@string/title_activity_latest_user_post"
            android:parentActivityName=".StageActivity">
        </activity>
    </application>

</manifest>
